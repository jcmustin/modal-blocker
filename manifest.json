{
  "name": "Modal Blocker",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A website blocker that can switch between modes",
  "homepage_url": "https://github.com/jcmustin/modal-blocker",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "permissions": [
    "tabs",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}